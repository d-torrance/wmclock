#!/bin/sh

set -e

case "$1" in
    remove)
        if [ -x /usr/bin/update-menus ]; then 
            update-menus; 
        fi
    ;;

    purge|upgrade|abort-install|abort-upgrade|disappear)
    ;;

    failed-upgrade)
        exit 1
    ;;

    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
